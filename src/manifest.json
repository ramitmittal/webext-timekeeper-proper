{
  "manifest_version": 2,
  "name": "Timekeeper",
  "description": "A simple stopwatch.",
  "version": "0.0.1",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Timekeeper",
    "default_popup": "popup/index.html"
  },
  "background": {
    "page": "background/index.html"
  }
}
